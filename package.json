{
  "name": "stylelint-tape",
  "version": "1.0.0",
  "description": "Test stylelint plugins",
  "author": "Jonathan Neal <jonathantneal@hotmail.com>",
  "license": "CC0-1.0",
  "repository": "csstools/stylelint-tape",
  "homepage": "https://github.com/csstools/stylelint-tape#readme",
  "bugs": "https://github.com/csstools/stylelint-tape/issues",
  "main": "index.js",
  "module": "index.mjs",
  "bin": {
    "stylelint-tape": "bin.js"
  },
  "files": [
    "index.js",
    "index.js.map",
    "index.mjs",
    "index.mjs.map",
    "bin.js",
    "bin.js.map"
  ],
  "scripts": {
    "build": "npm run build:api && npm run build:bin",
    "build:api": "rollup -c .rollup.js --silent",
    "build:bin": "rollup -c .rollup.bin.js --silent && prepend bin.js \"#!/usr/bin/env node\n\n\"",
    "prepublishOnly": "npm test",
    "pretest": "npm run build",
    "test": "npm run test:js && npm run test:node && npm run test:cli",
    "test:js": "eslint lib/*.js --cache --ignore-path .gitignore --quiet",
    "test:node": "node test/test.js",
    "test:cli": "stylelint-tape --cwd ./test"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "log-update": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.6.1",
    "eslint-config-dev": "^2.0.0",
    "pre-commit": "^1.2.2",
    "prepend-file-cli": "^1.0.6",
    "rollup": "^0.66.2",
    "rollup-plugin-babel": "^4.0.3",
    "stylelint": "^9.6.0"
  },
  "peerDependencies": {
    "stylelint": "^9"
  },
  "eslintConfig": {
    "extends": "dev",
    "parser": "babel-eslint"
  },
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "test",
    "tool"
  ]
}
